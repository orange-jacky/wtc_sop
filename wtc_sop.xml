<module>
    <name>wtc_sop</name>
    <filter>
        <port>58878</port>
        <port>49001</port>        
    </filter>
    <input>
    	 <context>
            <item>
                <name>tcp.srcport</name>
                <type>uint16</type>
            </item>
            <item>
                <name>tcp.dstport</name>
                <type>uint16</type>
            </item>
            <item>
                <name>ipv4.srcaddr</name>
                <type>ipv4</type>
            </item>            
            <item>
                <name>ipv4.dstaddr</name>
                <type>ipv4</type>
            </item>  
            <item>
                <name>ipv4.origsrcaddr</name>
                <type>ipv4</type>
            </item>            
            <item>
                <name>ipv4.origdstaddr</name>
                <type>ipv4</type>
            </item>        
        </context>  
         
				<metrics>          
            <item>
                <name>tcp.response_time.total</name>
                <type>uint64</type>
            </item>
            <item>
                <name>tcp.response_time.server</name>
                <type>uint64</type>
            </item>            
            <item>
                <name>tcp.response_time.network</name>
                <type>uint64</type>
            </item>
            <item>
                <name>tcp.response_time.load</name>
                <type>uint64</type>
            </item> 
        </metrics>    	
    </input>
    <output>
        <context>
            <item>
                <name>transaction.name</name>
                <type>string</type>
            </item>  
     	     <item>
                <name>status.code</name>
                <type>int16</type>
            </item>            
            <item>
                <name>status.isbad</name>
                <type>int16</type>
            </item>  
                        
        </context>
    </output>
</module>
